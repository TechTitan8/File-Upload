{
    "builds": [
        {
            "src": "fileupload/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "fileupload/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".:$PYTHONPATH"
    }
}